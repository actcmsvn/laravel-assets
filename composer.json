{
    "name": "actcmsvn/assets",
    "description": "Laravel assets management",
    "license": "MIT",
    "keywords": [
        "actcmsvn",
        "actcmsvn cms",
        "actcmsvn platform",
        "actcmsvn assets"
    ],
    "homepage": "https://actcmsvn.io.vn",
    "authors": [
        {
            "name": "ACTCMS Vietnam",
            "email": "info@actcms.io.vn"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0|^8.1|^8.2|^8.3",
        "laravel/framework": "^5.6|^6|^7|^8|^9|^10|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0|^8.0|^9.0|^10.0|^11.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ACTCMS\\Assets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ACTCMS\\Assets\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ACTCMS\\Assets\\Providers\\AssetsServiceProvider"
            ],
            "aliases": {
                "Assets": "ACTCMS\\Assets\\Facades\\AssetsFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}